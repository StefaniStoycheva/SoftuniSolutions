namespace fifth
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int targetSum = int.Parse(Console.ReadLine());
            int sum = 0;

            while (sum < targetSum)
            {
                int currentNum = int.Parse((Console.ReadLine()));
                sum += currentNum;
            }
            Console.WriteLine(sum);
        }
    }
}
